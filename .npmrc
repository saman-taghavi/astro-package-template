strict-peer-dependencies=false
auto-install-peers=true
# Set URL for your scoped packages.
# For example package with name `@my-org/my-package` will use this URL for download
@iranicard:registry=https://git.plzdev.ir/api/v4/projects/293/packages/npm/

# Add the token for the scoped packages URL. This will allow you to download
# `@my-org/` packages from private projects.
//git.plzdev.ir/api/v4/packages/npm/:_authToken="${NPM_TOKEN}"

# Add token for uploading to the registry. NPM_TOKEN should be set in enviornment
# with the project you want your package to be uploaded to.
//git.plzdev.ir/api/v4/projects/293/packages/npm/:_authToken="${NPM_TOKEN}"
